  {
    "description": "this is no parameters get method",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "text": "this is no parameter method没有参数哦哦哦",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
