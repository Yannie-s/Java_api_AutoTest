      "uri": "/getpara",
      "method": "get",
      "queries": {
        "name": "rabbit",
        "age": "13"
      }
    },
    "response": {
      "text": "存在参数的请求get",
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      }
    }
  }
